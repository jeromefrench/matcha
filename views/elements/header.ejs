<!DOCTYPE html>
<html>
	<head>
		<title>
			<% if (locals.title) {%>
				<%= locals.title %>
			<% }else{%>
				<%= "Matcha"%>
			<%}%>
		</title>

		<link rel="icon" type="image/png" sizes="192x192"  href="/assets/ico/android-icon-192x192.png">
		<link rel="icon" type="image/png" sizes="32x32" href="/assets/ico/favicon-32x32.png">
		<link rel="icon" type="image/png" sizes="96x96" href="/assets/ico/favicon-96x96.png">
		<link rel="icon" type="image/png" sizes="16x16" href="/assets/ico/favicon-16x16.png">

		<link rel="stylesheet" href="http://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
		<script src="https://code.jquery.com/jquery-1.12.4.js"></script>
		<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
		<link rel="stylesheet" href="/assets/semantic/dist/semantic.min.css">
		<script src="/assets/semantic/dist/semantic.min.js"></script>

	</head>
	<body class="Site"   onresize="myFunction()" >

		<style>

#moi1{
	color: rgb(0, 181, 173);
	background-color:black;
	font-weight: 900;
}

#moi2{
	color: white;
	background-color:black;
}

#moi3{
	color: white;
	background-color:black;
}
#moi4{
	color: white;
	background-color:black;
}

		</style>

		<div class="ui stackable container menu" style="background-color:black; ">
			<a href="/" class="item" id="moi1">Matcha</a>
			<a href="/about-you" id="moi2" class="item">About you</a>
			<a href="/research/?page=1" id="moi3" class="item">Research</a>
			<a href="/dashboard" id="moi4" class="item">Dashboard</a>
			<div class="right menu">
				<% if (locals.login) {%>
					<a href="/notifications" class="item" id="moi4" ><% if (bell == 1) { %> ðŸ”” <% } %>Notifications</a>
					<a href="/my-account" class="item" id="moi4" >My profile <%= locals.login %></a>
				<%}%>
				<div class="ui teal buttons" style="height:40px; margin-top:auto; margin-bottom:auto;">
					<div class="ui simple dropdown icon button">
						<% if (locals.logon) {%>
							<i style="height:30px;" class="sign out alternate icon"></i>
						<% }else{ %>
							<i class="sign in alternate icon"></i>
						<% } %>
						<div class="menu" >
							<% if (!locals.logon) { %>
								<% if (locals.title == "Sign Up") {} else{ %>
									<div class="item" ><a href="/sign-up" style="color:black"><b>Sign up</b></a></div>
								<% } %>
								<% if (locals.title == "Sign In") {} else{ %>
									<div class="item" ><a href="/sign-in" style="color:black"><b>Sign in</b></a></div>
								<% } %>
							<% }else{ %>
								<div class="item" href="/sign-out" style="color:black"><b>Sign out</b></a></div>
							<% } %>
						</div>
					</div>
				</div>
			</div>
		</div>



		<div id="myself" class="ui container">

		</div>


		<script>
			function myFunction() {
				var w = window.outerWidth;
				if(w <= 767){
					document.getElementById("myself").style.marginBottom = "250px";
				}
				else{
					document.getElementById("myself").style.marginBottom = "0px";
				}
			}

document.addEventListener('DOMContentLoaded', (event) => {
	myFunction();
})

		</script>

		<div class="Site-content">
