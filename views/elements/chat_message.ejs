<div class="comment">
	<a class="avatar">
		<img src="/assets/avatar/jenny.jpg">
	</a>
	<div class="content">
		<a class="author"><%= message.author %></a>
		<div class="metadata">
			<span class="date"><%= message.data_stamp %></span>
		</div>
		<div class="text">
			<%= message.message_content %>
		</div>
	</div>
</div>
